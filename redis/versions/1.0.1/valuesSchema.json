{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "required": [],
  "properties": {
    "name": {
      "type": "string",
      "default": "redis-cluster"
    },
    "gvc": {
      "type": "string",
      "default": "redis-cluster-example"
    },
    "replicas": {
      "type": "number",
      "default": 6
    },
    "port": {
      "type": "number",
      "default": 6379
    },
    "location": {
      "type": "string"
    },
    "diskCapacity": {
      "type": "number",
      "default": 20
    },
    "memory": {
      "type": "string",
      "default": "250Mi"
    },
    "cpu": {
      "type": "string",
      "default": "200m"
    },
    "internalFirewall": {
      "inboundAllowType": {
        "type": "string",
        "default": "same-org"
      }
    }
  }
}
